						"exec": [
							"// Dapatkan data respons",
							"const response = pm.response.json();",
							"",
							"// Periksa apakah respons memiliki properti 'access_token'",
							"if (response.data.token.access_token) {",
							"    // Set variabel environment dengan nilai access_token",
							"    pm.globals.set(\"TOKEN\", response.data.token.access_token);",
							"    console.log(\"Access token berhasil disimpan ke environment variable.\");",
							"} else {",
							"    console.error(\"Access token tidak ditemukan dalam respons.\");",
